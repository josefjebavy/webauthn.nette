parameters:
	siteName: Nette Webauthn

application:
	errorPresenter: Error
	mapping:
		*: App\*Module\Presenters\*Presenter

database:
	dsn: 'sqlite:/app/.data/main.sqlite3'

di:
	export:
		parameters: no
		tags: no

session:
	debugger: true
	expiration: 14 days


services:
	- App\Lib\SiteName(%siteName%)
	- App\Lib\WebauthnAuthenticator

	signinFormFactory:
		class: App\Forms\SignInFormFactory

	router: App\Router\RouterFactory::createRouter
